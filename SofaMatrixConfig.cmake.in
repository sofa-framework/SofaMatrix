# CMake package configuration file for the plugin @PROJECT_NAME@

@PACKAGE_GUARD@
@PACKAGE_INIT@

set(SOFAMATRIX_HAVE_SOFAGUI @SOFAMATRIX_HAVE_SOFAGUI@)
set(SOFAMATRIX_HAVE_QT5CORE @SOFAMATRIX_HAVE_QT5CORE@)

sofa_find_package(Sofa.Core QUIET REQUIRED)
sofa_find_package(Sofa.Component.Constraint.Lagrangian.Solver QUIET REQUIRED)
sofa_find_package(Sofa.Component.LinearSolver.Direct QUIET REQUIRED)
sofa_find_package(CImgPlugin QUIET REQUIRED)
sofa_find_package(Eigen3 QUIET REQUIRED)

if(SOFAMATRIX_HAVE_SOFAGUI)
    find_package(SofaGui QUIET REQUIRED)
endif()
if(SOFAMATRIX_HAVE_QT5CORE)
    find_package(Qt5 COMPONENTS Core QUIET REQUIRED)
endif()

if(NOT TARGET @PROJECT_NAME@)
    include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")
endif()

check_required_components(@PROJECT_NAME@)
